{
    "name": "QuibaX/laravel-vue-i18n-generator",
    "license": "MIT",
    "description": "Generates a vue-i18n compatible include file from your Laravel translations.",
    "keywords": ["laravel","vue-i18n"],
    "authors": [
        {
            "name": "Martin Lindhe",
            "email": "martin@ubique.se"
        },
        {
            "name": "Stijn Debakker",
            "email": "stijn@quibax.com"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "illuminate/console": "~7.0|~8.0",
        "illuminate/support": "~7.0|~8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7"
    },
    "autoload": {
        "psr-4": {
            "QuibaX\\VueInternationalizationGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "QuibaX\\VueInternationalizationGenerator\\GeneratorProvider"
            ]
        }
    }
}
